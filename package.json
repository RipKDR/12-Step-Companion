  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "seed:steps": "tsx server/scripts/seed-steps.ts",
    "seed:sample": "tsx server/scripts/seed-sample-data.ts",
    "test:setup": "tsx scripts/test-setup.ts",
    "verify": "tsx scripts/verify-setup.ts",
    "validate": "tsx scripts/validate-build.ts",
    "prebuild": "pnpm run type-check",
    "build:web": "pnpm --filter 12-step-companion-web build",
    "build:turbo": "turbo run build",
    "dev:web": "pnpm --filter 12-step-companion-web dev",
    "dev:turbo": "turbo run dev",
    "mobile:dev": "pnpm --filter mobile start",
    "mobile:ios": "pnpm --filter mobile ios",
    "mobile:android": "pnpm --filter mobile android",
    "mobile:web": "pnpm --filter mobile web",
    "type-check": "pnpm exec tsc --noEmit -p apps/web/tsconfig.json"
  },
